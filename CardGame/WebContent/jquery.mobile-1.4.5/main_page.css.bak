}

.circle {
/* 	background:rgb(48, 41, 41); */
	border-radius: 100%;
	cursor: pointer;
	position: relative;
	margin: 0 auto;
	width: 3.1em; height: 3.1em;
	overflow: hidden;
	font-weight: bold;
	font-size: 3em;
	line-height: 3.1em;
	text-align: center;
	text-transform: uppercase;
	
	background-color: #e74c3c;
	border-color: #7f2921;

}


.circle a {
   font-family: 'Monoton', cursive;
	color:#fff;
	text-decoration: none;
}


		